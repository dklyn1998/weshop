page {
  background-color: #F3F3F3;
}
.manin_area {
  display: flex;
  flex-direction: column;
}
.manin_area .user_info {
  display: flex;
  margin: 100rpx 0 30rpx 50rpx;
}
.manin_area .user_info image {
  width: 100rpx;
  border-radius: 50%;
}
.manin_area .user_info .user_name_id {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  margin-left: 20rpx;
}
.manin_area .user_info .user_name_id text:nth-of-type(1) {
  font-size: 32rpx;
  color: #3A3A3A;
}
.manin_area .user_info .user_name_id text:nth-of-type(2) {
  font-size: 26rpx;
  color: #8F7D6D;
}
.manin_area .app_info {
  display: flex;
  justify-content: space-around;
  height: 120rpx;
  background-color: rgba(255, 255, 255, 0.35);
}
.manin_area .app_info .app_info_area {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.manin_area .app_info .app_info_area text {
  font-size: 36rpx;
  color: #313131;
}
.manin_area .my_order {
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  margin-top: 50rpx;
}
.manin_area .my_order text:nth-of-type(1) {
  font-size: 30rpx;
  color: #313131;
}
.manin_area .my_order text:nth-of-type(2) {
  font-size: 28rpx;
  color: #ACACAC;
}
.manin_area .order_info {
  display: flex;
  justify-content: space-around;
  height: 100rpx;
  background-color: #fff;
}
.manin_area .order_info .order_info_area {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.manin_area .order_info .order_info_area text {
  font-size: 26rpx;
  color: #313131;
}
.manin_area .my_suver {
  padding: 20rpx;
  font-size: 30rpx;
  background-color: #fff;
}
.manin_area .my_suver_info navigator {
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  width: 100%;
  height: 90rpx;
  background-color: #fff;
  border: 1rpx soild #ECECEC;
}
.manin_area .my_suver_info navigator .my_suver_info_img {
  width: 29rpx;
}
.manin_area .my_suver_info navigator .my_suver_info_msg {
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #4F4F4F ;
  align-items: center;
  width: 100%;
  margin-left: 20rpx;
}
.manin_area .my_suver_info navigator image {
  width: 27rpx;
  height: 21rpx;
}
.manin_area .kefu_server {
  display: flex;
  align-items: center;
  width: 100%;
  height: 90rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  margin: 30rpx 0;
  background-color: #fff;
}
.manin_area .kefu_server .kefu_suver_info_img {
  width: 35rpx;
}
.manin_area .kefu_server .kefu_server_info {
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #4F4F4F ;
  align-items: center;
  width: 100%;
  margin-left: 20rpx;
  position: relative;
}
.manin_area .kefu_server image {
  width: 27rpx;
  height: 21rpx;
}
.manin_area .kefu_server button {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  min-height: 40rpx;
  height: 50rpx;
  background-color: rgba(255, 255, 255, 0);
}
