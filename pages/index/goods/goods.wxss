.bread_list {
  width: 100%;
  height: 75rpx ;
  background-color: #37383b;
}
.bread_list text {
  color: #666666;
  font-size: 30rpx;
  line-height: 75rpx;
  margin: 0 30rpx ;
}
.bread_list .active {
  color: #FCFBF9;
}
.goods_info {
  margin: 0 auto ;
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
  width: 95%;
  flex-wrap: wrap;
}
.goods_info navigator {
  width: 48%;
  height: 380rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  margin-bottom: 20rpx;
}
.goods_info navigator image {
  width: 100%;
}
.goods_info navigator text:nth-of-type(1) {
  color: #666666;
  font-size: 28rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.goods_info navigator text:nth-of-type(2) {
  color: #FF0060;
  font-size: 24rpx;
}
